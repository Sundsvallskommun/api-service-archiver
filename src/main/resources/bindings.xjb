<?xml version="1.0" encoding="UTF-8"?>
<jaxb:bindings version="2.1"
               xmlns:jaxb="http://java.sun.com/xml/ns/jaxb"
               xmlns:xs="http://www.w3.org/2001/XMLSchema">

    <jaxb:globalBindings>
        <jaxb:javaType name="java.time.LocalDateTime" xmlType="xs:dateTime" parseMethod="se.sundsvall.util.DateAdapter.parseDateTime" printMethod="se.sundsvall.util.DateAdapter.printLocalDateTime"/>
        <jaxb:javaType name="java.time.LocalDate" xmlType="xs:date" parseMethod="se.sundsvall.util.DateAdapter.parseDate" printMethod="se.sundsvall.util.DateAdapter.printLocalDate"/>
    </jaxb:globalBindings>
</jaxb:bindings>